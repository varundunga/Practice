package restApi;

import io.restassured.RestAssured;
import io.restassured.http.ContentType;
import io.restassured.response.Response;

import static io.restassured.RestAssured.given;
import static org.hamcrest.Matchers.*;

public class basics1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		RestAssured.baseURI = "https://www.googleapis.com/customsearch/v1";
		 Response res=
		given().param("key", "AIzaSyA56tTynxxy-7kEmRiAU1A56dk9ZRFmmBw").param("cx", "013955379900195856281:quje7hymsfw")
				.param("q", "platini").when().get("").then().assertThat().statusCode(200).and()
				.contentType(ContentType.JSON).and().body("kind", equalTo("customsearch#search")).and()
				.body("context.title", equalTo("Espnfc.in")).and().header("Server", equalTo("GSE1")).extract().response();
		 System.out.println(res.toString());
		 //res.toString();

	}

}
